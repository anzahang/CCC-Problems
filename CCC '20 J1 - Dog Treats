import java.util.Scanner;
public class DogTreats {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
				
		int S = sc.nextInt();
		int M = sc.nextInt();
		int L = sc.nextInt();
		int total = 0;
		
		total = S + (2*M) + (3 * L);
		
		if(total < 10) {
			System.out.println("sad");
		}else {
			System.out.println("happy");
		}

	}

}
