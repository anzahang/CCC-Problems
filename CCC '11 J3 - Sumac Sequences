import java.util.ArrayList;
import java.util.Scanner;

public class SumacSequence {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		int c = 1;
		int temp;
		
		while(b >= 0) {
			temp = a;
			a = b;
			b = temp-b;
			c++;
		}
			
		System.out.println(c);
	}

}
