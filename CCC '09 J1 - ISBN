import java.util.Scanner;

public class ISBN {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int total = 9*1+7*3+8*1+0*3+9*1+2*3+1*1+4*3+1*1+8*3;

		for (int i = 0; i < 3; i++) {
			int a = sc.nextInt();
			if (i % 2 == 0) {
				total += a;
			} else {
				total += a * 3;
			}

		}

		System.out.println("The 1-3-sum is " + total);

	}

}
